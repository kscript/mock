  "bugs": {
    "url": "https://github.com/kscript/mock/issues"
  },
  "homepage": "https://github.com/kscript/mock#readme",
  "dependencies": {
    "json-server": "^0.14.0",
    "mockjs": "^1.0.1-beta3",
    "path": "^0.12.7",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "*",
    "typescript": "^3.4.5"
